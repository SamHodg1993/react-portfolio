    {
      "command": ":vs[plit] file",
      "description": "Open a file in a new buffer and vertically split window"
    },
    {
      "command": ":vert[ical] ba[ll]",
      "description": "Edit all buffers as vertical windows"
    },
    {
      "command": ":tab ba[ll]",
      "description": "Edit all buffers as tabs"
    },
    {
      "command": "Ctrl + ws",
      "description": "Split window"
    },
    {
      "command": "Ctrl + wv",
      "description": "Split window vertically"
    },
    {
      "command": "Ctrl + ww",
      "description": "Switch windows"
    },
    {
      "command": "Ctrl + wq",
      "description": "Quit a window"
    },
    {
      "command": "Ctrl + wx",
      "description": "Exchange current window with next one"
    },
    {
      "command": "Ctrl + w=",
