    {
      "command": ":b[uffer]#",
      "description": "Go to a buffer by index #"
    },
    {
      "command": ":b[uffer] file",
      "description": "Go to a buffer by file"
    },
    {
      "command": ":ls or :buffers",
      "description": "List all open buffers"
